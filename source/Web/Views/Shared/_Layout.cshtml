                                    <div id="tabs-2">
                                        @Html.Partial("_RealtySearchContent")
                                    </div>
                                    <div id="tabs-3">
                                        @Html.Partial("_RealtySearchContent")
                                    </div>
                                    <div id="tabs-4">
                                        @Html.Partial("_RealtySearchContent")
                                    </div>
                                </div>
                            </div>
