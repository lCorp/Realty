        </div>
        <div class="web-footer">
            @Html.Partial("_WebFooter")
        </div>
    </div>
    @Html.Partial("_Social")
    @*@Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)
</body>
</html>
